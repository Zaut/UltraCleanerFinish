<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.UltraCleanerFinish" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimaryDark">@color/background</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/background</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.UltraCleanerFinish.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="Theme.UltraCleaner.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.UltraCleaner.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Test2" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/background</item>
        <item name="colorPrimaryDark">@color/background</item>
        <item name="colorAccent">@color/black</item>
    </style>

    <style name="Test" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/background_activities</item>
        <item name="colorPrimaryDark">@color/background_activities</item>
        <item name="colorAccent">@color/black</item>
    </style>
</resources>