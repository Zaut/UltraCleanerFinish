<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_activities"
    android:orientation="vertical"
    tools:context=".Home">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fr_place"
            android:name="com.example.ultracleanerfinish.FHome"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="@dimen/_10sdp"
        android:layout_gravity="center"
        android:gravity="center"
        tools:layout_editor_absoluteX="1dp">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageButton
                android:id="@+id/redbtnrocket"
                android:layout_width="@dimen/_49sdp"
                android:layout_height="@dimen/_59sdp"
                android:layout_weight="1"
                android:background="@drawable/btnmenu"
                android:backgroundTint="@color/blue"
                android:onClick="onClick"
                android:src="@drawable/ic_rocket_red" />

            <ImageView
                android:id="@+id/warningrocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:src="@drawable/ic_warning" />

            <ImageButton
                android:id="@+id/homebtn"
                android:layout_width="@dimen/_49sdp"
                android:layout_height="@dimen/_59sdp"
                android:layout_weight="1"
                android:background="@drawable/btnmenu"
                android:backgroundTint="@color/blue"
                android:onClick="onClick"
                android:src="@drawable/ic_rocket_white"
                android:visibility="invisible"
                tools:layout_editor_absoluteX="31dp"
                tools:layout_editor_absoluteY="615dp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/redbtn_battery"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_20sdp">

            <ImageButton
                android:id="@+id/btn_battery_red"
                android:layout_width="@dimen/_49sdp"
                android:layout_height="@dimen/_59sdp"
                android:layout_weight="1"
                android:background="@drawable/btnmenu"
                android:backgroundTint="@color/white"
                android:onClick="onClick"
                android:src="@drawable/ic_battery_red" />

            <ImageView
                android:id="@+id/warningbattery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:src="@drawable/ic_warning" />

            <ImageButton
                android:id="@+id/batterybtn"
                android:layout_width="@dimen/_49sdp"
                android:layout_height="@dimen/_59sdp"
                android:layout_weight="1"
                android:background="@drawable/btnmenu"
                android:backgroundTint="@color/white"
                android:onClick="onClick"
                android:src="@drawable/ic_battery_blue"
                android:visibility="invisible"
                tools:layout_editor_absoluteX="130dp"
                tools:layout_editor_absoluteY="615dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_20sdp">
            <ImageButton
                android:id="@+id/redbtn_fan"
                android:layout_width="@dimen/_49sdp"
                android:layout_height="@dimen/_59sdp"
                android:layout_weight="1"
                android:background="@drawable/btnmenu"
                android:backgroundTint="@color/white"
                android:onClick="onClick"
                android:src="@drawable/ic_fan_red" />

            <ImageView
                android:id="@+id/warningcpu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:src="@drawable/ic_warning" />

            <ImageButton
                android:id="@+id/fanbtn"
                android:layout_width="@dimen/_49sdp"
                android:layout_height="@dimen/_59sdp"
                android:layout_weight="1"
                android:background="@drawable/btnmenu"
                android:backgroundTint="@color/white"
                android:onClick="onClick"
                android:src="@drawable/ic_fan_blue"
                android:visibility="invisible"
                tools:layout_editor_absoluteX="238dp"
                tools:layout_editor_absoluteY="610dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_20sdp">

            <ImageButton
                android:id="@+id/redbtn_trash"
                android:layout_width="@dimen/_49sdp"
                android:layout_height="@dimen/_59sdp"
                android:layout_weight="1"
                android:background="@drawable/btnmenu"
                android:backgroundTint="@color/white"
                android:onClick="onClick"
                android:src="@drawable/ic_trash_red" />

            <ImageView
                android:id="@+id/warningtrash"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:src="@drawable/ic_warning" />

            <ImageButton
                android:id="@+id/trashbtn"
                android:layout_width="@dimen/_49sdp"
                android:layout_height="@dimen/_59sdp"
                android:layout_weight="1"
                android:background="@drawable/btnmenu"
                android:backgroundTint="@color/white"
                android:onClick="onClick"
                android:src="@drawable/ic_trash_blue"
                android:visibility="invisible"
                tools:layout_editor_absoluteX="336dp"
                tools:layout_editor_absoluteY="608dp" />
        </RelativeLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>